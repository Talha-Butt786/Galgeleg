<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/win_lose_background"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="411dp"
        android:layout_height="150dp"
        android:layout_marginTop="16dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toTopOf="@+id/imageView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/tryagain_background" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:fontFamily="@font/sancreek"
        android:gravity="center"
        android:text="s195475 Talha Butt\nGalgeleg"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="#351F02"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintStart_toStartOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="@+id/imageView11" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="150dp"
        android:layout_height="150dp"
        app:layout_constraintBottom_toTopOf="@+id/single_id"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView11"
        app:srcCompat="@drawable/hangman" />

    <ImageButton
        android:id="@+id/single_id"
        android:layout_width="180dp"
        android:layout_height="80dp"
        android:background="@drawable/mainbutton_background"
        app:layout_constraintBottom_toTopOf="@+id/multi_id"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
         />

    <ImageButton
        android:id="@+id/multi_id"
        android:layout_width="180dp"
        android:layout_height="80dp"
        android:background="@drawable/mainbutton_background"
        app:layout_constraintBottom_toTopOf="@+id/hjælp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/single_id"
         />

    <ImageButton
        android:id="@+id/hjælp"
        android:layout_width="180dp"
        android:layout_height="80dp"
        android:layout_marginBottom="32dp"
        android:scaleType="centerCrop"
        android:background="@drawable/mainbutton_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/multi_id"
         />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sancreek"
        android:text="SPIL"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/single_id"
        app:layout_constraintEnd_toEndOf="@+id/single_id"
        app:layout_constraintStart_toStartOf="@+id/single_id"
        app:layout_constraintTop_toTopOf="@+id/single_id" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:fontFamily="@font/sancreek"
        android:text="High Scores"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/multi_id"
        app:layout_constraintEnd_toEndOf="@+id/multi_id"
        app:layout_constraintStart_toStartOf="@+id/multi_id"
        app:layout_constraintTop_toTopOf="@+id/multi_id" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sancreek"
        android:text="Rules"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/hjælp"
        app:layout_constraintEnd_toEndOf="@+id/hjælp"
        app:layout_constraintStart_toStartOf="@+id/hjælp"
        app:layout_constraintTop_toTopOf="@+id/hjælp" />

</androidx.constraintlayout.widget.ConstraintLayout>